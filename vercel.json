{
    "name":"netflix mern",
    "version":2,
    "builds": [
        {"src": "server.js" 
        ,"use":"@vercel/node"
    },
    {
        "src": "src/**/*",
        "use":"@vercel/static"
    }
    ],
    "routes":[
        {
        "src":"/(.*)",
        "dest": "/server.js",
        "methods":["GET","POST","PUT","DELETE","PATCH","OPTION"],
        "headers":{
            "Access-Control-Allow-Origin":"*"
        }
    }
    ]

}